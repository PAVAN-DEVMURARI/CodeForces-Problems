#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define mod 1000000007
void solve()
{
    int a ;
    cin >> a;
    string str1(a, ' ');
    cin >> str1;
    int b;
    cin >> b;
    string str2(b, ' ');
    cin >> str2;
    string str3(b, ' ');
    cin >> str3;

    


    for (int i=0;i<b;i++)
    {
        if (str3[i] == 'D')
        {
            str1.push_back(str2[i]);
        }   
        else
        {
             str1.insert(str1.begin(), str2[i]);
        }
    }

    cout << str1 << endl;


}
int main() {
    ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
// #ifndef ONLINE_JUDGE
//     freopen("input.txt", "r", stdin);
//     freopen("output.txt", "w", stdout);
// #endif

    ll test=1;
    cin>>test;
    while(test--)
    {
        solve();
    }
    return 0;
}
